{
  "name": "mitsift",
  "display-name": "Mitsift Sift",
  "description": "",
  "author": "",
  "version": "1.0.0",
  "icon": "assets/blueprint.svg",
  "interfaces": {
    "email": {
      "thread" : {
        "controller": "dist/js/controller.umd-es2015.min.js"
      },
      "client" : {
        "controller": "dist/js/email-client-controller.umd-es2015.min.js"
      }
    },
    "summary": {
      "controller": "dist/js/controller.umd-es2015.min.js"
    }
  },
  "dag": {
    "inputs":{
      "emails":{
        "gmailEmails":{
          "filter":{
            "conditions":[{
              "from":{
                "regexp": {
                  "pattern": ".*@gmail\\.com",
                  "flags": "i"
                }
              }
            },{
              "date": "between now and 1 year before now"
            }],
            "operator": "AND"
          },
          "wants": [
            "archive",
            "textBody",
            "strippedHtmlBody"
          ]
        }
      }
    },
    "nodes":[{
      "#": "Parse",
      "implementation": {
        "javascript": "server/parse.js"
      },
      "input": {
          "bucket": "gmailEmails"
      },
      "outputs": {
          "messageSummarys": {}
      }
    }],
    "stores":{
    },
    "outputs":{
      "exports":{
        "messageSummarys" : {
          "key$schema":"string"
        }
      }
    }
  }
}
